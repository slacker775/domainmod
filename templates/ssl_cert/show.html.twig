{% extends 'base.html.twig' %}

{% block title %}SslCert{% endblock %}

{% block body %}
    <h1>SslCert</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ssl_cert.id }}</td>
            </tr>
            <tr>
                <th>Ip</th>
                <td>{{ ssl_cert.ip }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ ssl_cert.name }}</td>
            </tr>
            <tr>
                <th>ExpiryDate</th>
                <td>{{ ssl_cert.expiryDate ? ssl_cert.expiryDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>TotalCost</th>
                <td>{{ ssl_cert.totalCost }}</td>
            </tr>
            <tr>
                <th>Notes</th>
                <td>{{ ssl_cert.notes }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ ssl_cert.status }}</td>
            </tr>
            <tr>
                <th>FeeFixed</th>
                <td>{{ ssl_cert.feeFixed ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('ssl_cert_index') }}">back to list</a>

    <a href="{{ path('ssl_cert_edit', {'id': ssl_cert.id}) }}">edit</a>

    {{ include('ssl_cert/_delete_form.html.twig') }}
{% endblock %}
