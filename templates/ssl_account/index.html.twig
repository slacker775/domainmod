{% extends 'base.html.twig' %}

{% block title %}SslAccount index{% endblock %}

{% block body %}
    <h1>SslAccount index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Owner</th>
                <th>SslProvider</th>
                <th>EmailAddress</th>
                <th>Username</th>
                <th>Password</th>
                <th>Reseller</th>
                <th>ResellerId</th>
                <th>Notes</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ssl_account in ssl_accounts %}
            <tr>
                <td>{{ ssl_account.id }}</td>
                <td>{{ ssl_account.owner }}</td>
                <td>{{ ssl_account.sslProvider }}</td>
                <td>{{ ssl_account.emailAddress }}</td>
                <td>{{ ssl_account.username }}</td>
                <td>{{ ssl_account.password }}</td>
                <td>{{ ssl_account.reseller ? 'Yes' : 'No' }}</td>
                <td>{{ ssl_account.resellerId }}</td>
                <td>{{ ssl_account.notes }}</td>
                <td>
                    <a href="{{ path('ssl_account_show', {'id': ssl_account.id}) }}">show</a>
                    <a href="{{ path('ssl_account_edit', {'id': ssl_account.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="14">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('ssl_account_new') }}">Create new</a>
{% endblock %}
