{% extends 'base.html.twig' %}

{% block title %}SslProvider{% endblock %}

{% block body %}
    <h1>SslProvider</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ssl_provider.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ ssl_provider.name }}</td>
            </tr>
            <tr>
                <th>Url</th>
