{% extends 'base.html.twig' %}

{% block title %}SslProvider index{% endblock %}

{% block body %}
    <h1>SslProvider index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Url</th>
                <th>Notes</th>
                <th>CreationTypeId</th>
                <th>CreatedBy</th>
                <th>Created</th>
                <th>Updated</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ssl_provider in ssl_providers %}
            <tr>
                <td>{{ ssl_provider.id }}</td>
                <td>{{ ssl_provider.name }}</td>
                <td>{{ ssl_provider.url }}</td>
                <td>{{ ssl_provider.notes }}</td>
                <td>{{ ssl_provider.creationTypeId ? 'Yes' : 'No' }}</td>
                <td>{{ ssl_provider.createdBy }}</td>
                <td>{{ ssl_provider.created ? ssl_provider.created|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ ssl_provider.updated ? ssl_provider.updated|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('ssl_provider_show', {'id': ssl_provider.id}) }}">show</a>
                    <a href="{{ path('ssl_provider_edit', {'id': ssl_provider.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('ssl_provider_new') }}">Create new</a>
{% endblock %}
