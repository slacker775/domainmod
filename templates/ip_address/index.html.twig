{% extends 'base.html.twig' %}

{% block title %}IpAddress index{% endblock %}

{% block body %}
    <h1>IpAddress index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Ip</th>
                <th>Rdns</th>
                <th>Notes</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ip_address in ip_addresses %}
            <tr>
                <td>{{ ip_address.id }}</td>
                <td>{{ ip_address.name }}</td>
                <td>{{ ip_address.ip }}</td>
                <td>{{ ip_address.rdns }}</td>
                <td>{{ ip_address.notes }}</td>
                <td>
                    <a href="{{ path('ip_address_show', {'id': ip_address.id}) }}">show</a>
                    <a href="{{ path('ip_address_edit', {'id': ip_address.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="8">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('ip_address_new') }}">Create new</a>
{% endblock %}
