{% extends 'base.html.twig' %}

{% block title %}IpAddress{% endblock %}

{% block body %}
    <h1>IpAddress</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ip_address.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ ip_address.name }}</td>
            </tr>
            <tr>
                <th>Ip</th>
                <td>{{ ip_address.ip }}</td>
            </tr>
            <tr>
                <th>Rdns</th>
                <td>{{ ip_address.rdns }}</td>
            </tr>
            <tr>
                <th>Notes</th>
                <td>{{ ip_address.notes }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ ip_address.created ? ip_address.created|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{{ ip_address.updated ? ip_address.updated|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('ip_address_index') }}">back to list</a>

    <a href="{{ path('ip_address_edit', {'id': ip_address.id}) }}">edit</a>

    {{ include('ip_address/_delete_form.html.twig') }}
{% endblock %}
